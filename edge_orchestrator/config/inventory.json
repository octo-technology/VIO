{
  "cameras": [
    "fake",
    "pi_camera",
    "usb_camera"
  ],
  "models": {
    "marker_quality_control": {
      "category": "classification",
      "version": 1,
      "class_names": [
        "OK",
        "KO"
      ],
      "image_resolution": [
        224,
        224
      ]
    },
    "pin_detection": {
      "category": "classification",
      "version": 1,
      "class_names": [
        "OK",
        "NOK"
      ],
      "image_resolution": [
        224,
        224
      ]
    },
    "mobilenet_ssd_v2_coco": {
      "category": "object_detection",
      "version": 1,
      "class_names_path": "coco_labels_originals.txt",
      "output": {
        "boxes_coordinates": "detection_boxes",
        "objectness_scores": "detection_scores",
        "detection_classes": "detection_classes"
      },
      "image_resolution": [
        300,
        300
      ],
      "objectness_threshold": 0.3
    },
    "mobilenet_ssd_v2_face": {
      "category": "object_detection",
      "version": 1,
      "class_names_path": "face_labels.txt",
      "output": {
        "boxes_coordinates": "detection_boxes",
        "objectness_scores": "detection_scores",
        "detection_classes": "detection_classes"
      },
      "image_resolution": [
        320,
        320
      ],
      "objectness_threshold": 0.3
    }
  },
  "camera_rules": [
    "expected_label_rule",
    "min_nb_objects_rule",
    "max_nb_objects_rule"
  ],
  "item_rules": [
    "threshold_ratio_rule",
    "min_threshold_KO_rule"
  ]
}
